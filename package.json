    "start": "node app.js"
  },
  "keywords": [],
  "author": "A_Zagorskiy",
  "devDependencies": {
    "@parcel/transformer-inline-string": "=2.8.2",
    "@parcel/transformer-sass": "=2.8.2",
    "express": "=4.18.2",
    "nanoid": "^4.0.1",
    "parcel": "=2.8.2",
    "parcel-transformer-hbs": "^1.0.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "handlebars": "=4.7.7",
    "sass": "=1.57.1"
  }
}
