{
  "name": "middle.messenger.praktikum.yandex",
  "version": "0.1.0",
  "description": "sprint-1",
  "default": "dist/index.html",
  "source": "static/index.html",
  "browserlist": "> 0.5%, last 2 version, not dead",
  "targets": {
    "global": {
      "context": "browser",
      "outputFormat": "global"
    }
  },
  "engines": {
    "node": ">= 14.0"
  },
  "scripts": {
    "tsc:w": "tsc -w",
    "dev": "parcel index.html --port 3000 --no-source-maps --open",
