<form class='registr' action='#/login'>
  <div class='wrapper__form wrapper-txt'>
    <label class='registr-txt'>Регистрация</label>
    {{{inputMail}}}
    <span class='for-error'>wrong email</span>
    {{{inputLogin}}}
    <span class='for-error'>bad login</span>
    {{{inputFname}}}
    <span class='for-error'></span>
    {{{inputSname}}}
    <span class='for-error'></span>
    {{{inputTel}}}
    <span class='for-error'>not a number</span>
    {{{inputPass}}}
    <span class='for-error'>to short password</span>
    {{{inputPassRep}}}
    <span class='for-error'>Пароли не совпадают</span>
  </div>
  <div class='wrapper__form wrapper-btn'>
    {{{button}}}
    <a href='#/login'>Войти</a>
  </div>
</form>
<nav class='links'>
  <a href='#/login'>Login</a>
  <a href='#/registration'>Registration</a>
  <a href='#/profile'>Profile</a>
  <a href='#/404'>Page not found</a>
  <a href='#/500'>Server error</a>
